{
  "env": {
    "server": {
      "presets": ["camo"]
    },
    "development": {
      "presets": ["next/babel"]
    }
  },
  "plugins": [
    ["babel-plugin-root-import"],
    ["babel-plugin-transform-object-rest-spread"],
    ["import", [
      {"libraryName": "antd"},
      {"libraryName": "lodash", "libraryDirectory": "/"}
    ]]
  ]
}
